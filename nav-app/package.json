{
  "name": "attack-navigator",
  "description": "Web app that provides basic navigation and annotation of ATT&CK matrices",
  "repository": {
    "type": "git",
    "url": "https://github.com/mitre-attack/attack-navigator.git"
  },
  "version": "4.7.1",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCI --code-coverage",
    "test:coverage": "ng test --code-coverage --watch=false",
    "test:watch": "ng test --watch=true",
    "lint": "ng lint",
  "e2e": "cypress run --headless",
  "e2e:ci": "cypress run --headless --browser chrome --reporter junit",
  "e2e:headless": "cypress run --headless",
  "e2e:open": "cypress open",
    "test:docker:unit": "docker compose -f ../docker-compose.test.yml up --abort-on-container-exit --exit-code-from app-test app-test",
    "test:docker:e2e": "docker compose -f ../docker-compose.test.yml --profile e2e up --abort-on-container-exit --exit-code-from e2e-tests",
    "test:docker:selenium": "docker compose -f ../docker-compose.test.yml --profile selenium up --abort-on-container-exit --exit-code-from selenium-tests",
    "test:docker:all": "docker compose -f ../docker-compose.test.yml --profile e2e --profile selenium up --abort-on-container-exit --exit-code-from e2e-tests",
    "test:selenium": "npm run build && npm run test:selenium:run",
    "test:selenium:run": "jest tests/selenium --testTimeout=30000",
    "test:selenium:grid": "npm run test:selenium -- --seleniumAddress=http://selenium-hub:4444/wd/hub",
    "test:all": "npm run test:ci && npm run e2e:ci && npm run test:selenium",
    "test:performance": "npm run build && npm run test:performance:run",
    "test:performance:run": "playwright test tests/performance",
    "postinstall": "node patch-webpack.js"
  },
  "dependencies": {
    "@angular/animations": "^14.2.10",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.10",
    "@angular/compiler": "^14.2.10",
    "@angular/core": "^14.2.10",
    "@angular/forms": "^14.2.10",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.10",
    "@angular/platform-browser-dynamic": "^14.2.10",
    "@angular/router": "^14.2.10",
    "@fontsource/roboto": "^4.2.3",
    "@fontsource/roboto-mono": "^4.2.2",
    "@types/file-saver": "^2.0.3",
    "core-js": "^3.26.1",
    "crypto-browserify": "^3.12.0",
    "d3": "^5.16.0",
    "d3-svg-legend": "^2.25.6",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "is_js": "^0.9.0",
    "load-json-file": "^7.0.1",
    "mathjs": "^11.3.3",
    "ngx-color-picker": "^13.0.0",
    "ngx-drag-drop": "^14.0.0",
    "ngx-markdown": "^14.0.1",
    "ngx-smart-popover": "^1.4.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "rxjs": "^6.6.7",
    "rxjs-compat": "^6.6.7",
    "stream-browserify": "^3.0.0",
    "tinygradient": "^1.1.5",
    "tslib": "^2.4.0",
    "util": "^0.12.5",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "14.1.0",
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular/cli": "^14.2.9",
    "@angular/compiler-cli": "^14.2.10",
    "@angular/language-service": "^14.2.10",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^17.0.32",
    "codelyzer": "^6.0.0",
    "cypress": "^12.17.4",
    "cypress-visual-regression": "^3.0.0",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.5",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "marked": "^4.2.2",
    "playwright": "^1.40.0",
    "@playwright/test": "^1.40.0",
    "selenium-webdriver": "^4.15.0",
    "ts-node": "~10.7.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4",
    "wait-on": "^7.2.0"
  },
  "optionalDependencies": {
    "fsevents": "2.1.2"
  }
}
